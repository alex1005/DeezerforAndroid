<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:ordering="sequentially">
    <objectAnimator
        android:propertyName="scaleY"
        android:duration="300"
        android:valueFrom=".4"
        android:valueTo=".6"
        android:valueType="floatType" />
    <objectAnimator
        android:propertyName="scaleY"
        android:duration="300"
        android:valueFrom=".6"
        android:valueTo=".4"
        android:valueType="floatType" />
    <objectAnimator
        android:propertyName="scaleY"
        android:duration="300"
        android:valueFrom=".4"
        android:valueTo=".8"
        android:valueType="floatType" />
    <objectAnimator
        android:propertyName="scaleY"
        android:duration="300"
        android:valueFrom=".8"
        android:valueTo=".4"
        android:valueType="floatType" />
</set>